{
  "PolicyName": "dynamodb-full-access",
  "PolicyDocument": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "DynamoDBAccess",
        "Effect": "Allow",
        "Action": [
          "dynamodb:*"
        ],
        "Resource": "arn:aws:dynamodb:us-west-2:123456789012:table/MyTable"
      }
    ]
  }
}
